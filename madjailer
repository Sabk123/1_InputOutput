public class madjailer {

    char cells = 100;
    boolean[] cellsopen = new boolean[cells]; //used boolean for true and false (locked and unlocked)

    public void initCode() {
        for (int i = 0; i < cells; i++) {
            cellsopen[i] = false; // This initializes the code and creates the first "time" the guard walks through
        }
    }

    public void aisleWalk() {
        int walk;
        for (walk = 1; walk <= cells; walk++)
            ; // the upcoming code will run (loop) until the "walk" variable is equal to the cells variable (100)
        for (int i = walk - 1; i < cells; i += walk) ;
        {
            cellsopen[i] = !cellsopen[i];
        }
    }

    public void cellOpened() {
        System.out.println("Cells");
        for (int i = 0; i < cells; i++) ;
        {
            if (cellsopen[i]) {
                System.out.println((i + 1) + " ");
            }

        }
    }
}
